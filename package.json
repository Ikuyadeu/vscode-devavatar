{
	"name": "devreplay",
	"displayName": "DevReplay",
	"description": "Devreplay for Visual Studio Code",
	"version": "1.1.9",
	"author": "Yuki Ueda",
	"publisher": "Ikuyadeu",
	"icon": "img/icon.png",
	"license": "SEE LICENSE IN LICENSE",
	"galleryBanner": {
		"color": "#FFFFFF",
		"theme": "light"
	},
	"keywords": [
		"Coding Style",
		"DevReplay"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/devreplay/vscode-devreplay"
	},
	"bugs": {
		"url": "https://github.com/devreplay/vscode-devreplay/issues"
	},
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other",
		"Linters",
		"Formatters"
	],
	"activationEvents": [
		"*"
	],
	"main": "./client/out/extension",
	"scripts": {
		"vscode:prepublish": "npm run webpack",
		"webpack": "webpack --mode production --config ./client/webpack.config.js",
		"webpack:dev": "npm run clean && webpack --mode none --config ./client/webpack.config.js",
		"compile": "tsc -b ./client/tsconfig.json",
		"watch": "tsc -w -p client/tsconfig.json",
		"clean": "rimraf client/out",
		"postinstall": "cd client && npm install && cd .."
	},
	"devDependencies": {
		"@types/node": "^13.11.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"eslint": "^6.8.0",
		"eslint-config-standard": "^14.1.1",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-jsdoc": "^21.0.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^4.0.1",
		"json-loader": "^0.5.7",
		"merge-options": "^2.0.0",
		"rimraf": "^3.0.2",
		"ts-loader": "^6.2.2",
		"typescript": "^3.8.3",
		"webpack": "^4.42.1",
		"webpack-cli": "^3.3.11"
	}
}
